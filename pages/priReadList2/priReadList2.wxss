/**index.wxss**/
@import "../templateDemo/templateDemo.wxss";
view, scroll-view, text, button {
  font-size: 32rpx;
}

.pri-read-list {
  background-color: #f1f1f1;
}

.head-swipe {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  padding: 0 40rpx;
  box-sizing: border-box;
  line-height: 100rpx;
  box-shadow: 0px 1px 10px 1px rgba(204, 204, 204, 0.2);
  overflow: hidden;
  position: relative;
}

/* tabÊ†è */

.fix-grade {
  position: absolute;
  top: 0;
  left: 40rpx;
  height: 100%;
  padding-right: 20rpx;
  border-right:1px solid #f9f9f9;
  /* box-shadow: 1px 0px 10px 1px rgba(204, 204, 204, 0.2); */
}

.scroll-grade {
  float: left;
  height: 100%;
  width: 100%;
  padding-left: 95rpx;
  overflow: hidden;
  box-sizing: border-box;
}

.grade-pack {
  width: 1280rpx;
  padding-left: 20rpx;
}

.scroll-grade .grade {
  float: left;
  width: 160rpx;
  text-align: center;
  height: 96rpx;
  border-bottom: 4px solid transparent;
}

.grade.current-grade {
  border-color: #f93;
}

.composition-list {
  width: 100%;
  padding: 40rpx 40rpx;
  overflow: hidden;
  box-sizing:border-box;
}

.composition-box {
  float: left;
  width:30%;
  padding:0 20rpx;
  margin-right:5%; 
  background-color: #fff;
  margin-bottom:40rpx;
  box-sizing:border-box;
}
.composition{
text-align:center;width:100%;height:100%;display: flex;
  flex-direction: column;
}
.composition text{display: inline-block;margin:auto auto;text-align:center;}
.composition-box:nth-child(3n){
  margin-right:0;
}

.composition-box{
  background:url(data:image/png;base64,) no-repeat;background-size:cover;
}

